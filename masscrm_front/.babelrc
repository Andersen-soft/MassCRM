{
  "presets": ["@babel/typescript", "@babel/react"],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-syntax-dynamic-import"
  ],
  "env": {
    "test": {
      "presets": [
        [
          "@babel/env",
          {
            "targets": {
              "node": "current"
            }
          }
        ]
      ]
    },
    "development": {
      "presets": [
        [
          "@babel/env",
          {
            "modules": false,
            "useBuiltIns": "entry",
            "corejs": 2
          }
        ]
      ]
    },
    "production": {
      "presets": [
        [
          "@babel/env",
          {
            "modules": false,
            "useBuiltIns": "entry",
            "corejs": 2
          }
        ]
      ]
    }
  }
}
